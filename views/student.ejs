  });
  </script>
      <div class="col-md-4 col-md-offset-4">
		<div>
			<p>Profile Detail</p>
		</div>
		<div class="Table">
			<center>
				<table class="table table-hover text-center">
					<tr><td>Name </td>
					<td>
						<%=name %>
					</td></tr>
					<tr><td>Email </td><td>
						<%= email %>
					</td></tr>
					<tr><td>PRN </td><td>
						<%= PRN %>
					</td></tr>
				</table>
			</center>
			<% if(locals.login){ %>
				<div class="alter alert-success text-center">
					<%= login %>
				</div>
			<% } %>
		</div>
	</div>
